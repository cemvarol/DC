a=$(az ad signed-in-user show --query userPrincipalName)
A=$(echo "$a" | sed -e 's/\(.*\)/\L\1/')
B=${A:$(echo `expr index "$A" @`)}
C=${B:: -24}
D=$(echo "$C"con01)
E=$(echo "$C"con02)
RG=Containers
L=FranceCentral


az group create --name $RG --location $L

az storage account create --name $D --resource-group $RG --kind Storage --location $L --sku Standard_LRS


az network nsg create -g $RG -n DCNSG
az network nsg rule create -g $RG --nsg-name DCNSG -n ConRule --priority 100 --destination-port-ranges "*" --direction Inbound


az network vnet create --resource-group $RG --name DC-VNet  --address-prefixes 10.207.0.0/16 --subnet-name SN01 --subnet-prefix 10.207.1.0/24

export SUBNETID=$(az network vnet subnet show --resource-group $RG --vnet-name DC-VNet --name SN01 --query id -o tsv)
export SUBNETN=$(az network vnet subnet show --resource-group $RG --vnet-name DC-VNet --name SN01 --query name -o tsv)

az network vnet subnet update -g $RG --vnet-name DC-VNet --name $SUBNETN --network-security-group DCNSG

az vm create --resource-group $RG --name FRC-DC01 --location $L --image Win2019DataCenter --admin-username cem --admin-password 1q2w3e4r5t6y* --size standard_D4s_v3 --public-ip-address "FRC-DC01-Pip" --subnet $SUBNETID --boot-diagnostics-storage $D --license-type Windows_Server --nsg ""


